<Application
    x:Class="JiraGraphThing.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ioc="using:JiraGraphThing.IOC"
    xmlns:con="using:JiraGraphThing.Converters"
    xmlns:model="using:JiraGraphThing.Models"
    xmlns:local="using:JiraGraphThing">

    <Application.Resources>
        <ResourceDictionary>
            <ioc:ViewModelLocator x:Key="Locator"/>
            <con:BooleanToVisibility x:Key="BooleanToVisibility"/>
            <con:BooleanToString x:Key="BooleanToString"/>
            <con:DateFormatter x:Key="DateFormatter"/>
            <con:CompactTimeFormatter x:Key="CompactTimeFormatter"/>
            <con:DoubleToInt x:Key="DoubleToInt"/>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
